{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
        [
          "module-resolver",
          {
            "root": [
              "./src"
            ],
            "alias": {
              "@containers": "./src/containers",
              "@router": "./src/router",
              "@pages": "./src/pages",
              "@layout": "./src/layout"
            }
          }
        ],
      [
        "react-hot-loader/babel"
      ],
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ],
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
}